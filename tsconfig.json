{
  
  "compilerOptions": {
    "target": "ESNext",
    "module": "commonjs",
    "rootDir": "./src",
    "outDir": "./build",
    "removeComments": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "strictNullChecks": true,
    "skipLibCheck": true
  },
  "include": ["src", "dist/depreciated/services/deepseek.ts", "dist/depreciated/services/flux.ts", "dist/depreciated/teste.ts"],
  "exclude": ["node_modules","**/*.spec.ts","dist","build"]
}
